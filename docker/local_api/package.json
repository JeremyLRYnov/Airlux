{
  "name": "nodejs-express-redis",
  "version": "1.0.0",
  "description": "Node.js Restful CRUD API with Node.js, Express and redis",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test:unit": "jest --testPathPattern=app/test --coverage",
    "lint:fix": "eslint --fix ./*.js app/**/*.js",
    "start": "node server.js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageProvider": "v8",
    "coverageReporters": [
      "text",
      "lcov",
      "clover",
      "text-summary",
      "html",
      "json",
      "html"
    ]
  },
  "keywords": [
    "nodejs",
    "express",
    "restapi"
  ],
  "author": "bezkoder",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-validator": "^6.14.3",
    "jsonwebtoken": "^9.0.0",
    "latest": "^0.2.0",
    "mqtt": "^5.0.5",
    "nodemon": "^2.0.20",
    "pure-rand": "^6.0.2",
    "redis-om": "^0.3.6",
    "ws": "^8.14.1"
  },
  "devDependencies": {
    "axios": "^1.3.4",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0"
  }
}
